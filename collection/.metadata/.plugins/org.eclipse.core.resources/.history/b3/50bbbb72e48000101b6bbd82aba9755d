package com.sn;

import java.util.Arrays;
import java.util.List;
import java.util.stream.Collectors;

public class StreamIntegerLambda {
	public static void main(String[] args) {
		List<Integer> intList = Arrays.asList(40,20,80,60,10,20);
		System.out.println(intList);
		
		intList.stream().filter(x -> x>40).forEach(System.out.::println);
	}
}
