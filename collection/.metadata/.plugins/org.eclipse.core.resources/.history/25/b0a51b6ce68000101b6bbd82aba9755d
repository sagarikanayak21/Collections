package com.sn;

import java.util.Arrays;
import java.util.List;
import java.util.stream.Collectors;

public class StreamIntegerLambda {
	public static void main(String[] args) {
		List<Integer> intList = Arrays.asList(40,20,80,60,10,20,33,87,46);
		System.out.println(intList);
		
//		intList.stream().filter(x -> x>40).forEach(System.out::println);
		
//		List<Integer> filterList = intList.stream().filter(x-> x>40).collect(Collectors.toList());
//		System.out.println(filterList);
		
//		intList.stream().filter(x -> x%2==1).forEach(System.out::println);
		
//		intList.stream().map(x -> x+10).forEach(System.out::println);
		
//		intList.stream().filter(x -> x<40).map(y -> y+10).forEach(System.out::println);
		
//		intList.stream().map(x -> x+10).filter(y -> y<40).forEach(System.out::println);
		
//		int sum = intList.stream().reduce((a,b) -> a+b).get();
//		System.out.println(sum);
		
		int sum = intList.stream().reduce(100, (a,b) -> a+b).get();
		System.out.println(sum);
		
		
	}
}
