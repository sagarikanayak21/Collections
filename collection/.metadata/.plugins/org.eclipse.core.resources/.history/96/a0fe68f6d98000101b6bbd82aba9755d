package com.sn;

import java.util.Arrays;
import java.util.List;
import java.util.stream.Collectors;

public class StreamStringListSort {
	public static void main(String[] args) {
		List<String> stringList = Arrays.asList("Lucky", "sagarika", "Mama", "Rupa");
		
//		System.out.println(stringList);
		
//		stringList.stream().forEach(System.out::print);	
		
//		stringList.stream().forEach(str -> {
//			System.out.print(str + " ");
//		});
		
		List<String> sortedList = stringList.stream().sorted().collect(Collectors.toList());
		System.out.println(sortedList);
	}
}
