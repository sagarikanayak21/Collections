package com.sn.empLst;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.Iterator;
import java.util.List;
import java.util.ListIterator;

import com.sn.entity.Employee;

public class EmployeeList {
	public static void main(String[] args) {
		
		Employee empObj = new Employee();
		empObj.setEmpId(101);
		empObj.setEmpName("Sagarika");
		empObj.setSalary(10000.0);
//		System.out.println(empObj);
		
		Employee empObj2 = new Employee();
		empObj2.setEmpId(102);
		empObj2.setEmpName("Sidhartha");
		empObj2.setSalary(20000.0);
		
		Employee empObj3 = new Employee();
		empObj3.setEmpId(103);
		empObj3.setEmpName("Mama");
		empObj3.setSalary(30000.0);
		
//		List<Employee> empList = new ArrayList<>(Arrays.asList(empObj, empObj2, empObj3));
		List<Employee> empList = new ArrayList<>();
		empList.add(empObj);
		empList.add(empObj2);
		empList.add(empObj3);
		System.out.println(empList);
	
//		ListIterator<Employee> itr = empList.listIterator();
//		while(itr.hasNext()) {
//			Employee emp = itr.next();
//			System.out.print(emp.getEmpId() + " ");
//			System.out.print(emp.getEmpName() + " ");
//			System.out.print(emp.getSalary() + " ");
//			System.out.println(" ");
//		}
		
		Iterator<Employee> itr = empList.iterator();
		while(itr.hasNext()) {
			Employee emp = itr.next();
			System.out.println(emp.getEmpId() + " ");
			System.out.println(emp.getEmpName() + " ");
			System.out.println(emp.getSalary() + " ");
		}
	}
	
}
